<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="match_parent"
    xmlns:foo="http://schemas.android.com/apk/res-auto"
    android:layout_height="match_parent"
    android:orientation="vertical"
    app:layout_behavior="@string/appbar_scrolling_view_behavior">
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/repeat">
    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_weight="1"
        android:orientation="horizontal"></LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_weight="1"
        android:orientation="horizontal">

        <layout.api.TextViewPlus
            android:gravity="right|center_vertical"
            android:text="ID"
            android:textSize="13dip"
            foo:customFont="InterparkGothicBold.ttf"
            android:textColor="#74406C"
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="3
            "/>
        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="0.5"></LinearLayout>

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="4">
            <layout.api.EditTextPlus
                android:id="@+id/sign_id"
                android:gravity="center_vertical"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:editable="true"
                android:inputType="text"
                android:imeOptions="actionNext"
                foo:customFont="InterparkGothicBold.ttf"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="3"></LinearLayout>



    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_weight="1"
        android:orientation="horizontal">


        <layout.api.TextViewPlus
            android:gravity="right|center_vertical"
            android:text="PASSWORD"
            android:textSize="13dip"
            foo:customFont="InterparkGothicBold.ttf"
            android:textColor="#74406C"
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="3"/>

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="0.5"></LinearLayout>

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="4">
            <layout.api.EditTextPlus
                android:id="@+id/sign_password"
                android:gravity="center_vertical"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:editable="true"
                android:inputType="text"
                android:imeOptions="actionNext"
                foo:customFont="InterparkGothicBold.ttf"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="3"></LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_weight="1"
        android:orientation="horizontal">


        <layout.api.TextViewPlus
            android:gravity="right|center_vertical"
            android:text="NAME"
            android:textSize="13dip"
            foo:customFont="InterparkGothicBold.ttf"
            android:textColor="#74406C"
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="3"/>

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="0.5"></LinearLayout>

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="4">
            <layout.api.EditTextPlus
                android:id="@+id/sign_name"
                android:gravity="center_vertical"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:editable="true"
                android:inputType="text"
                android:imeOptions="actionNext"
                foo:customFont="InterparkGothicBold.ttf"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="3"></LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_weight="1"
        android:orientation="horizontal">

        <layout.api.TextViewPlus
            android:gravity="right|center_vertical"
            android:text="PERSONAL NUMBER"
            android:layout_width="0px"
            android:textSize="13dip"
            foo:customFont="InterparkGothicBold.ttf"
            android:textColor="#74406C"
            android:layout_height="fill_parent"
            android:layout_weight="3"/>

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="0.5"></LinearLayout>

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="3">
            <layout.api.EditTextPlus
                android:id="@+id/sign_person_h"
                android:gravity="center_vertical"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:editable="true"
                android:inputType="text"
                android:imeOptions="actionNext"
                foo:customFont="InterparkGothicBold.ttf"/>
        </LinearLayout>

        <layout.api.TextViewPlus
            android:gravity="center"
            android:text="-"
            android:textSize="40dip"
            foo:customFont="InterparkGothicBold.ttf"
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="0.5"/>

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="3">
            <layout.api.EditTextPlus
                android:id="@+id/sign_person_t"
                android:gravity="center_vertical"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:editable="true"
                android:inputType="text"
                android:imeOptions="actionNext"
                foo:customFont="InterparkGothicBold.ttf"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="0.5"></LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_weight="1"
        android:orientation="horizontal">
        <layout.api.TextViewPlus
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="3"
            android:text="GENDER"
            android:textSize="13dip"
            foo:customFont="InterparkGothicBold.ttf"
            android:textColor="#74406C"
            android:gravity="right|center_vertical"/>

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="0.5"></LinearLayout>

        <RadioGroup
            android:id="@+id/sign_gender"
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="7"
            android:orientation="horizontal">
            <layout.api.RadioPlus
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="3"
                android:text="MALE"
                android:textSize="13dip"
                foo:customFont="InterparkGothicBold.ttf" />

            <layout.api.RadioPlus
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="3"
                android:text="FEMALE"
                android:textSize="13dip"
                foo:customFont="InterparkGothicBold.ttf" />

        </RadioGroup>

    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_weight="1"
        android:orientation="horizontal">

        <layout.api.TextViewPlus
            android:gravity="right|center_vertical"
            android:text="PHONE"
            android:textSize="13dip"
            foo:customFont="InterparkGothicBold.ttf"
            android:textColor="#74406C"
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="3"/>

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="0.5"></LinearLayout>

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="1.7">
            <layout.api.EditTextPlus
                android:id="@+id/sign_phone_h"
                android:gravity="center_vertical"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:editable="true"
                android:inputType="text"
                android:imeOptions="actionNext"
                foo:customFont="InterparkGothicBold.ttf"/>
        </LinearLayout>

        <layout.api.TextViewPlus
            android:gravity="center"
            android:text="-"
            android:textSize="40dip"
            foo:customFont="InterparkGothicBold.ttf"
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="0.6"/>

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="1.7">
            <layout.api.EditTextPlus
                android:id="@+id/sign_phone_m"
                android:gravity="center_vertical"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:editable="true"
                android:inputType="text"
                android:imeOptions="actionNext"
                foo:customFont="InterparkGothicBold.ttf"/>
        </LinearLayout>

        <layout.api.TextViewPlus
            android:gravity="center"
            android:text="-"
            android:textSize="40dip"
            foo:customFont="InterparkGothicBold.ttf"
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="0.6"/>

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="1.7">
            <layout.api.EditTextPlus
                android:id="@+id/sign_phone_t"
                android:gravity="center_vertical"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:editable="true"
                android:inputType="text"
                android:imeOptions="actionNext"
                foo:customFont="InterparkGothicBold.ttf"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="0.5"></LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_weight="1"
        android:orientation="horizontal">

        <layout.api.TextViewPlus
            android:gravity="right|center_vertical"
            android:text="EMAIL"
            android:textSize="13dip"
            foo:customFont="InterparkGothicBold.ttf"
            android:textColor="#750061"
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="3"/>

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="0.5"></LinearLayout>

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="2.5">
            <layout.api.EditTextPlus
                android:id="@+id/sign_email_h"
                android:gravity="center_vertical"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:editable="true"
                android:inputType="text"
                android:imeOptions="actionNext"
                foo:customFont="InterparkGothicBold.ttf"/>
        </LinearLayout>

        <layout.api.TextViewPlus
            android:gravity="center"
            android:text=" @ "
            android:textSize="20dip"
            foo:customFont="InterparkGothicBold.ttf"
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="0.65"/>

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="3.8"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="8dp">
            <Spinner
                android:id="@+id/sign_email_t"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:entries="@array/array"></Spinner>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="0.5"></LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_weight="1"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="1">
        </LinearLayout>

        <layout.api.ButtonPlus
            android:id="@+id/sign_signup"
            android:text="SIGN UP"
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="5"
            android:onClick="onClick"
            android:background="#74406C"
            foo:customFont="InterparkGothicBold.ttf"
            android:textColor="#ffffff"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"/>

        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="1">
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_weight="3.5"
        android:orientation="horizontal">

    </LinearLayout>

</LinearLayout>
