<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:foo="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/repeat">
    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_weight="1"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:orientation="vertical">
            <layout.api.TextViewPlus
                android:id="@+id/regist_name"
                android:gravity="center"
                android:text="Store Name"
                android:textSize="20dp"
                android:layout_width="fill_parent"
                android:layout_height="0px"
                android:layout_weight="1"
                android:orientation="horizontal"
                android:textColor="#74406C"
                foo:customFont="InterparkGothicBold.ttf"></layout.api.TextViewPlus>
            <layout.api.TextViewPlus
                android:id="@+id/regist_address"
                android:gravity="center"
                android:text="Address"
                android:textSize="20dp"
                android:layout_width="fill_parent"
                android:layout_height="0px"
                android:layout_weight="1"
                android:orientation="horizontal"
                android:textColor="#74406C"
                foo:customFont="InterparkGothicBold.ttf"></layout.api.TextViewPlus>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="0.1"
            android:orientation="vertical"></LinearLayout>
        <LinearLayout
            android:layout_width="0px"
            android:layout_height="fill_parent"
            android:layout_weight="0.9"
            android:orientation="vertical">
            <ImageView
                android:id="@+id/regist_image"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:src="@drawable/bg"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_weight="1.5"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="0px"
                android:layout_weight="1"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="0px"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"></LinearLayout>
                <layout.api.TextViewPlus
                    android:gravity="left|center_vertical"
                    android:text="Menu"
                    android:layout_width="0px"
                    android:textSize="15dp"
                    android:textColor="#74406C"
                    android:layout_height="fill_parent"
                    foo:customFont="InterparkGothicBold.ttf"
                    android:layout_weight="13"></layout.api.TextViewPlus>
                <LinearLayout
                    android:layout_width="0px"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"></LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="0px"
                android:layout_weight="4"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="0px"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"></LinearLayout>
                <TableLayout
                    android:id="@+id/regist_menu"
                    android:layout_width="0px"
                    android:layout_height="fill_parent"
                    android:layout_weight="13"
                    android:background="#FFFFFF">
                    <TableRow>
                        <layout.api.TextViewPlus
                            android:id="@+id/table_id_h"
                            android:layout_weight="1"
                            android:text="Picture"
                            android:textSize="17dp"
                            android:background="#74406C"
                            android:textColor="#ffffff"
                            android:gravity="center"
                            foo:customFont="InterparkGothicBold.ttf"/>
                        <layout.api.TextViewPlus
                            android:id="@+id/table_id_m"
                            android:layout_weight="1"
                            android:text="Item"
                            android:textSize="17dp"
                            android:background="#74406C"
                            android:textColor="#ffffff"
                            android:gravity="center"
                            foo:customFont="InterparkGothicBold.ttf"/>
                        <layout.api.TextViewPlus
                            android:id="@+id/table_id_t"
                            android:layout_weight="1"
                            android:text="Price"
                            android:textSize="17dp"
                            android:background="#74406C"
                            android:textColor="#ffffff"
                            android:gravity="center"
                            foo:customFont="InterparkGothicBold.ttf"/>
                    </TableRow>
                </TableLayout>
                <LinearLayout
                    android:layout_width="0px"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"></LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_weight="1.5"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="0px"
                android:layout_weight="1"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="0px"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"></LinearLayout>
                <layout.api.TextViewPlus
                    android:gravity="left|center_vertical"
                    android:text="Review"
                    android:layout_width="0px"
                    android:textSize="15dp"
                    android:textColor="#74406C"
                    android:layout_height="fill_parent"
                    foo:customFont="InterparkGothicBold.ttf"
                    android:layout_weight="13"></layout.api.TextViewPlus>
                <LinearLayout
                    android:layout_width="0px"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"></LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="0px"
                android:layout_weight="4"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="0px"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"></LinearLayout>
                <TableLayout
                    android:id="@+id/regist_review"
                    android:layout_width="0px"
                    android:layout_height="fill_parent"
                    android:layout_weight="13"
                    android:background="#FFFFFF">
                    <TableRow>
                        <layout.api.TextViewPlus
                            android:id="@+id/table_review_1"
                            android:layout_weight="1"
                            android:text="Grade"
                            android:textSize="17dp"
                            android:background="#74406C"
                            android:textColor="#ffffff"
                            android:gravity="center"
                            foo:customFont="InterparkGothicBold.ttf"/>
                        <layout.api.TextViewPlus
                            android:id="@+id/table_id_2"
                            android:layout_weight="1"
                            android:text="Review"
                            android:textSize="17dp"
                            android:background="#74406C"
                            android:textColor="#ffffff"
                            android:gravity="center"
                            foo:customFont="InterparkGothicBold.ttf"/>
                        <layout.api.TextViewPlus
                            android:id="@+id/table_id_3"
                            android:layout_weight="1"
                            android:text="Writer"
                            android:textSize="17dp"
                            android:background="#74406C"
                            android:textColor="#ffffff"
                            android:gravity="center"
                            foo:customFont="InterparkGothicBold.ttf"/>
                        <layout.api.TextViewPlus
                            android:id="@+id/table_id_4"
                            android:layout_weight="1"
                            android:text="Date"
                            android:textSize="17dp"
                            android:background="#74406C"
                            android:textColor="#ffffff"
                            android:gravity="center"
                            foo:customFont="InterparkGothicBold.ttf"/>
                    </TableRow>
                </TableLayout>
                <LinearLayout
                    android:layout_width="0px"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"></LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0px"
        android:layout_weight="0.5"
        android:orientation="horizontal"></LinearLayout>
</LinearLayout>
